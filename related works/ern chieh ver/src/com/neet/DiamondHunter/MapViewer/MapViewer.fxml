<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="598.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.neet.DiamondHunter.MapViewer.MapViewerController">

    <Canvas fx:id="canvas" focusTraversable="true" height="640.0" layoutX="162.0" onMouseClicked="#setLocation" onMouseMoved="#highlightCursor" width="640.0" AnchorPane.leftAnchor="162.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
   <VBox layoutX="-2.0" layoutY="-1.0" prefHeight="640.0" prefWidth="163.0" styleClass="slide" stylesheets="@MapViewer.css">
      <children>
         <Button fx:id="btnAxe" mnemonicParsing="false" onMouseClicked="#onBtnAxeClicked" prefHeight="128.0" prefWidth="163.0" styleClass="slide" stylesheets="@MapViewer.css">
            <graphic>
               <ImageView fitHeight="56.0" fitWidth="74.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="axes.png" />
                  </image>
               </ImageView>
            </graphic>
         </Button>
         <Button fx:id="btnBoat" mnemonicParsing="false" onMouseClicked="#onBtnBoatClicked" prefHeight="128.0" prefWidth="163.0" styleClass="slide">
            <graphic>
               <ImageView fitHeight="56.0" fitWidth="74.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="yatch.png" />
                  </image>
               </ImageView>
            </graphic>
         </Button>
         <Button mnemonicParsing="false" onAction="#resetToDefaultCoordinates" prefHeight="128.0" prefWidth="163.0" styleClass="slide">
            <graphic>
               <ImageView fitHeight="56.0" fitWidth="74.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="settings.png" />
                  </image>
               </ImageView>
            </graphic>
         </Button>
         <Button mnemonicParsing="false" onAction="#undoChanges" prefHeight="128.0" prefWidth="163.0" styleClass="slide">
            <graphic>
               <ImageView fitHeight="56.0" fitWidth="74.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="curve-arrow.png" />
                  </image>
               </ImageView>
            </graphic>
         </Button>
         <Button mnemonicParsing="false" onAction="#showInformation" prefHeight="128.0" prefWidth="163.0" styleClass="slide">
            <graphic>
               <ImageView fitHeight="56.0" fitWidth="74.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="icon.png" />
                  </image>
               </ImageView>
            </graphic>
         </Button>
      </children>
   </VBox>


</AnchorPane>
